{
    "name": "daniwebdev/php-websocket-app",
    "description": "Just websocket app",
    "version": "v0.0.1",
    "repositories": {
        "phocket/framework": {
            "type": "path",
            "url": "./packages/phocket-framework"
        }
    },
    "require": {
        "illuminate/database": "^12.10",
        "phocket/framework": "dev-main"
    },
    "require-dev": {
        "pestphp/pest": "^3.8"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "daniwebdev",
            "email": "dani.webdev@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "test": "pest",
        "test:coverage": "pest --coverage"
    }
}