{
    "name": "daniwebdev/php-websocket-app",
    "description": "Just websocket app",
    "version": "v0.0.1",
    "require": {
        "illuminate/database": "^12.10",
        "phrity/websocket": "^3.4",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "pestphp/pest": "^3.8"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "daniwebdev",
            "email": "dani.webdev@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "autoload": {
        "psr-4": {
            "IOC\\Websocket\\": "core/",
            "App\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "test": "pest",
        "test:coverage": "pest --coverage"
    }
}